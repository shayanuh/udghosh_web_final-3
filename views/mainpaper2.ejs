    function Jumptoq(event)
    {
        var qno = event.target.value;
        for(var i = 0 ;i<45;i++)
        {
            document.getElementsByClassName("question")[i].classList.add("hidden")
        }
        document.getElementById(qno).classList.remove("hidden")
    }
    function next(event)
    {
     var qno =event.target.name;
     for(var i = 0 ;i<45;i++)
        {
            document.getElementsByClassName("question")[i].classList.add("hidden")
        }
        document.getElementById(qno).classList.remove("hidden") 
    }
    function clears(event)
    {
        
        
        var qno=parseInt(event.target.name[2])
        if (event.target.name.length>3)
        qno=parseInt(event.target.name[2]+event.target.name[3])
        for(var i =0 ; i<4 ;i++)
        {
            document.getElementsByClassName("qindex")[0].getElementsByTagName("input")[qno-1].classList.remove("attempted");
            document.getElementsByClassName("question")[qno-1].getElementsByTagName("input")[i].checked =false;
        }
    }
    function attempt()
    {
        for (var i =0 ;i<45;i++)
        {
            var q = document.getElementsByClassName("question")[i];
            console.log( document.getElementsByClassName("qindex")[0].getElementsByTagName("input")[2])
            for(var j =0 ;j<4;j ++)
            {
                if(q.getElementsByTagName("input")[j].checked === true)
                {
                  document.getElementsByClassName("qindex")[0].getElementsByTagName("input")[i].classList.add("attempted");  
                }
            }
        }
    }
</script>
